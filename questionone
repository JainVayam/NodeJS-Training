function secondHighest(s) {
  var largest,secondLarge=0;
  var arraySize = s.length;
  largest = s[0];
  for (var index = 0; index < arraySize; index++) {
    if (s[index] > largest) {
      secondLarge = largest;
      largest = s[index];
    }
  }
  return secondLarge;
}
var points = [40, 100, 1, 5, 25, 10]; 
console.log(secondHighest(points));
