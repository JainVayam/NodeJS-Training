function printchecker() {
 	var dict2 = { 
		i: 1, 
		like: 2, 
		samsung: 3, 
		mobile: 4, 
		cecream: 7, 
		man: 8, 
		go: 9
	};
	var inputs = "icecream";
	var t = inputs.length;
	var count = 0;
	var answer = "" ;
	for (var inputindex = 0; inputindex < t; inputindex++) {
		for (var secondindex = inputindex; secondindex < t; secondindex++) {
			var selection = inputs.slice(inputindex, secondindex + 1);
			for(var key in dict2) {
					if (key == selection) {
						answer = answer + selection;
						console.log(answer);
						count = count + dict2[key];
						break;
					} 
			}
				if (selection == key) {
					console.log("breaks"); 
					break;
				}
		}
		if (answer == inputs && answer.length == t ) {
			break;
		}
	} 
	if (answer == inputs) {
		answer = "yes";
		console.log(count);
		console.log(answer);
	} else {
			answer = "no";
			console.log(answer);
		}
} 
printchecker();